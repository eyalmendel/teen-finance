{
  "expo": {
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "experiments": {
      "tsconfigPaths": true
    },
    "name": "teen-finance",
    "slug": "teen-finance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#f2f2fa"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.teenfinance.app"
    },
    "android": {
      "package": "com.teenfinance.app"
    },
    "web": {
      "favicon": "./assets/icons/logo.png"
    },
    "plugins": [
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": false,
          "supportsPictureInPicture": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7a90ad14-92c7-4841-8e4a-489ffef01f4e"
      }
    }
  }
}
