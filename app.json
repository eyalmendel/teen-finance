{
  "expo": {
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "experiments": {
      "tsconfigPaths": true
    },
    "name": "teen-finance",
    "slug": "teen-finance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/logo.png",
      "resizeMode": "contain",
      
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {},
    "web": {
      "favicon": "./assets/icons/logo.png"
    },
    "plugins": [
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": false,
          "supportsPictureInPicture": true
        }
      ]
    ]
  }
}
