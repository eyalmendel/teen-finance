{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@screens/*": ["app/screens/*"],
      "@components/*": ["app/components/*"],
      "@models/*": ["app/models/*"],
      "@config/*": ["app/config/*"],
      "@enums/*": ["app/enums/*"],
      "@services/*": ["app/services/*"],
      "@utils/*": ["app/utils/*"],
      "@context/*": ["app/context/*"],
      "@hooks/*": ["app/hooks/*"],
      "@assets/*": ["assets/*"]
    }
  }
}
